{
  "name": "meeting-keeper",
  "description": "Keep track of who comes to your meetings privately!",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:style": "postcss ./src/components/index.css --output ./src/index.css",
    "build:script": "rollup --config rollup.config.ts --configPlugin typescript",
    "build": "npm run build:script; npm run build:style",
    "watch:style": "npm run build:style -- --watch",
    "watch:script": "npm run build:script -- --watch",
    "watch": "npm run watch",
    "serve": "serve ./src/",
    "start": "concurrently 'npm run watch' 'npm run serve'"
  },
  "dependencies": {
    "@types/simple-icons": "^5.0.0",
    "@types/simple-peer": "^9.11.0",
    "simple-icons": "^5.3.0",
    "simple-peer": "^9.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "cssnano": "^5.0.6",
    "eslint": "7.29.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.52.2",
    "serve": "^12.0.0",
    "typescript": "4.3.4"
  }
}
